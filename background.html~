<html>
<head>
<script>
  chrome.tabs.onUpdated.addListener(
  function(tabId, changeInfo, tab) 
  {
    if(changeInfo.status == "complete") {
      if(tab.url == "http://www.google.com/") {
        console.log(tab.url);
        chrome.tabs.executeScript(null, 
{code:"document.body.bgColor='red'"});
      }
    }
  });
</script>
</head>
</html>
