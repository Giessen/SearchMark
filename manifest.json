{
  "name": "SearchMark",
  "version": "1.0",
  "description": "Search your bookmarked pages.",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Search Bookmarked Pages",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "http://*/*",
    "https://*/*",
    "unlimited_storage"
  ]
}